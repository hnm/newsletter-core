{
	"name" : "hnm/newsletter-core",
	"type" : "n2n-module",
	"description" : "Newsletter core module for n2n",
	"keywords" : [
		"n2n-module",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Thomas GÃ¼nther",
			"email" : "guenther@hnm.ch"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n": "~7.3",
		"n2n/n2n-composer-module-installer" : "^7.1.5",
		"n2n/rocket": "^2.0.12",
		"swiftmailer/swiftmailer" : "v6.0.2"
	},
	"require-dev" : {
        "phpunit/phpunit" : "^9.5"
	},
	"autoload" : {
		"psr-4" : {
			"newsletter\\core\\" : "src/app/newsletter/core"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"newsletter\\core\\" : "src/test/newsletter/core"
		}
	}
}
